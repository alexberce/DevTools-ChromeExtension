  "browser_action": {
    "default_icon": "src/icons/icon512.png",
    "default_title": "__MSG_extName__",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "idle",
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "storage"
  ],
  "omnibox": {
    "keyword": "dev"
  }
}